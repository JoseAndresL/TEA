# Ejemplos de mejores practicas al nombrar variables (dos formatos)

# Ejemplos de nombres de variables utilizando pothole_case_naming
temp_celsius = 26.67
primer_nombre = "Jose"


# Ejemplos de nombres de variables utilizando camelCase
clase = "TEA"
tempFarenheit = 80.0
primerNombre = "Jose"
apellido = "Laborda"
